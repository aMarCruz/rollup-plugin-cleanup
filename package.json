{
  "name": "rollup-plugin-cleanup",
  "version": "1.0.0",
  "description": "Rollup plugin to trim trailing spaces, compact empty lines, and normalize line endings",
  "main": "dist/rollup-plugin-cleanup.js",
  "jsnext:main": "dist/rollup-plugin-cleanup.es.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec test/index.js",
    "lint": "node-version-gte-4 && eslint src test/index.js || node-version-lt-4",
    "pretest": "rollup -c",
    "build": "rm -rf dist/* && rollup -c",
    "prepublish": "npm run lint && rm -rf dist/* && npm test"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aMarCruz/rollup-plugin-cleanup"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "javascript",
    "es2015",
    "es6",
    "modules",
    "cleanup"
  ],
  "author": {
    "name": "aMarCruz",
    "email": "amarcruzbox-git@yahoo.com",
    "url": "https://github.com/aMarCruz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aMarCruz/rollup-plugin-cleanup/issues"
  },
  "homepage": "https://github.com/aMarCruz/rollup-plugin-cleanup",
  "dependencies": {
    "acorn": "^4.0.4",
    "magic-string": "^0.19.0",
    "rollup-pluginutils": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^3.13.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "node-version-check": "^2.1.1",
    "rollup": "^0.41.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-riot": "^1.1.0"
  }
}
